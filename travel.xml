---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>In Place, Of Food</title>
    <description>Travelogue wherein I narrate about a food and the place it's at</description>
    <link>https://owenwang.com/travel</link>
    {% for post in site.posts %}
        {% if post.publish and post.categories contains 'travel' %}
        <item>
          <title>{{ post.title | xml_escape }}</title>
          <description>{{ post.content | xml_escape }}</description>
          <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
          <link>https://owenwang.com{{ post.url}}</link>
          <guid isPermaLink="true">https://owenwang.com{{ post.url}}</guid>
        </item>
        {% endif %}
    {% endfor %}
  </channel>
</rss>
